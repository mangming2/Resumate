{
    "manifest_version": 3,
    "name": "Resumate",
    "version": "1.0.0",
    "description": "Resumate is a tool that helps you create a resume.",
    "permissions": [
        "sidePanel",
        "tabs"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "side_panel": {
        "default_path": "index.html"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ]
}